{
    "name": "thejandroman-nc_nginx",
    "version": "0.1.0",
    "author": "thejandroman",
    "summary": "Install and configure Nginx according to NC's requirements.",
    "license": "Apache-2.0",
    "source": "git@github.com:thejandroman/puppet-nc_nginx.git",
    "project_page": "https://github.com/thejandroman/puppet-nc_nginx",
    "issues_url": "https://github.com/thejandroman/puppet-nc_nginx/issues",
    "dependencies": [
        {
            "name": "puppetlabs-stdlib",
            "version_requirement": ">= 1.0.0"
        },
        {
            "name": "puppetlabs-vcsrepo",
            "version_requirement": ">= 1.3.1"
        },
        {
            "name": "jfryman-nginx",
            "version_requirement": ">= 0.2.7"
        }
    ],
    "operatingsystem_support": [
        {
            "operatingsystem": "Ubuntu",
            "operatingsystemrelease": [
                "12.04",
                "14.04"
            ]
        },
        {
            "operatingsystem": "RedHat",
            "operatingsystemrelease": [
                "6",
                "7"
            ]
        },
        {
            "operatingsystem": "CentOS",
            "operatingsystemrelease": [
                "6",
                "7"
            ]
        }
    ]
}
